@model MVC_SkillWidgets.Models.Siniflar.Yetenek

@{
    ViewBag.Title = "YetenekEkle";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}

    <form class="form-group" method="post">
        <div style="margin-bottom:18px">
            <label>Yetenek Adı</label>
            @Html.TextAreaFor(x => x.YetenekAd, new { @class = "form-control" })
        </div>
        <div style="margin-bottom:18px">
            <label>Değer</label>
            @Html.TextAreaFor(x => x.Deger, new { @class = "form-control" })
        </div>
        <button class="btn btn-primary">Kaydet</button>
    </form>


